{
  "name": "pink",
  "version": "13.0.0",
  "private": true,
  "description": "Личный проект «Пинк» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "^7.2.6",
    "browser-sync": "^2.23.6",
    "gulp": "3.9.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-sass": "^3.1.0",
    "stylelint": "8.4.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'source/sass/**/*.scss' --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.9"
  },
  "dependencies": {
    "del": "^3.0.0",
    "error": "^7.0.2",
    "gulp-autopolyfiller": "^1.2.1",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "gulp-webp": "^3.0.0",
    "js": "^0.1.0",
    "plumber": "^0.4.8",
    "postcss-object-fit-images": "^1.1.2",
    "posthtml-include": "^1.1.0",
    "pump": "^3.0.0",
    "run-sequence": "^2.2.1"
  }
}
